{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "X-Accel-Buffering",
          "value": "no"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-transform"
        }
      ]
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 300
    },
    "app/api/refine/run/route.ts": {
      "maxDuration": 300
    }
  }
}

